
#include <stdio.h>

int main()
{
     
  int nombre, a, temp, som=0;  
  
  printf(" Entrez un nombre entier quelconque : ");
  scanf("%d", &nombre);  
  
  temp=nombre;  
  
  while(nombre>0)  
  { 
    a=nombre%10;  
    som=som+(a*a*a);  
    nombre=nombre/10;  
  }  
  if(temp==som)  
    printf(" %d est un nombre Armstrong",temp);  
  else  
    printf(" %d n'est pas un nombre Armstrong",temp);  

    return 0;
}
